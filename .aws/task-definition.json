                {
                    "containerPort": 8001,
                    "hostPort": 8001,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 8020,
                    "hostPort": 8020,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 8030,
                    "hostPort": 8030,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 8040,
